<div>
<table class = 'table'>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

    <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%#= post.id %></td>
        <td><%#= post.content %></td>
        <td><%#= render "post", :locals => {:post => post}%></td>
        <td><%#= render :partial => @posts %></td>
        <td><%= render @posts %></td>

<%#= render 'form', airport: @airport %>

      </tr>
    <% end %>
  </tbody>

</table>
</div>